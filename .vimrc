colorscheme desert
syntax on
set tabstop=4
set softtabstop=4
set number
set showcmd
set cursorline
filetype indent on
set wildmenu
set wildmode=list:longest
set lazyredraw
set showmatch
set incsearch
set hlsearch
set foldenable
set foldlevelstart=10
nnoremap <space> za
set foldmethod=indent
set history=1000



